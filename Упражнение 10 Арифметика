import math 
a=int(input("Введите число а "))#ввод числа а
b=int(input("Введите число b "))#ввод числа b
print("Сумма a и b = ", a+b) #вывод суммы чисел
print("Разница между а и b = ", a-b)#вывод разницы
print("Произведение a и b = ", a*b)#вывод произведения
print("Частное от деления a на b = ", a/b)#вывод частного
print("Остаток от деления a на b = ", a%b)#вывод остатка
print("десятичный логарифм числа а = ",math.log10(a))#вывод логарифма
print("возведение а в степень b = ", a**b)#вывод степени